{
  "name": "games",
  "version": "0.1.0",
  "private": true,
  "config": {
    "registry": "vselemanov/nngame-front"
  },
  "author": {
    "name": "Vladislav Selemanov & Co"
  },
  "description": "Битва за Нижний",
  "scripts": {
    "depstart": "BROWSER=none react-scripts start",
    "dep-build": "react-scripts build && semver -i $(cat version.num) > version.num && sudo docker build -t $npm_package_config_registry:$(cat version.num) . && sudo docker push $npm_package_config_registry:$(cat version.num)",
    "dep-test": "react-scripts test",
    "dep-eject": "react-scripts eject",
    "build-main": "cross-env NODE_ENV=production webpack --config webpack.main.prod.config.js",
    "build-renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.prod.config.js",
    "build": "npm run build-main && npm run build-renderer",
    "start-renderer-dev": "webpack-dev-server --config webpack.renderer.dev.config.js",
    "start-main-dev": "webpack --config webpack.main.config.js && electron ./dist/main.js",
    "start-dev": "cross-env START_HOT=1 npm run start-renderer-dev",
    "prestart": "npm run build",
    "start": "electron .",
    "lint": "eslint --ext=jsx,js,tsx,ts src",
    "test": "jest '(\\/test\\/(?!e2e/)).*'",
    "pretest:e2e": "npm run build",
    "test:e2e": "jest '(\\/test\\/e2e/).*'",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder -c.extraMetadata.main=./dist/main.js",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "build": {
    "productName": "BattleNN",
    "appId": "battle.nn",
    "directories": {
      "output": "release"
    },
    "extends": null,
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json",
      "./dist/main.js"
    ],
    "mac": {
      "target": "dmg",
      "category": "game"
    },
    "linux": {
      "target": "deb"
    },
    "win": {
      "target": "portable"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-modules-commonjs": "^7.10.1",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/history": "^4.7.2",
    "@types/jest": "24.0.15",
    "@types/node": "12.0.10",
    "@types/react": "16.8.22",
    "@types/react-dom": "16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-select": "^3.0.0",
    "@types/react-table": "^6.8.4",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "electron": "^9.0.0",
    "electron-builder": "^22.7.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-webpack": "^2.8.2",
    "electron-webpack-ts": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "source-map-loader": "^1.0.0",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "axios": "^0.19.0",
    "babel-loader": "^8.1.0",
    "connected-react-router": "^6.4.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "history": "^4.9.0",
    "jwt-decode": "^2.2.0",
    "moment": "^2.24.0",
    "nes": "^10.0.2",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.4.1",
    "react-select": "^3.0.4",
    "react-table": "^6.10.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^8.0.2",
    "semver": "^6.1.2",
    "style-loader": "^1.2.1",
    "typescript": "3.4.5"
  }
}
